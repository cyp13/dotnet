#设置项目访问路径前缀
server.servlet.context-path=/rule
server.port=8081
#回话超时时间，以秒为单位
server.servlet.session.timeout=3000
#数据源配置
spring.datasource.url=jdbc:mysql://192.168.5.164:3306/examine_rule?useUnicode=true&characterEncoding=UTF-8&allowMultiQueries=true&autoReconnect=true&zeroDateTimeBehavior=convertToNull
spring.datasource.driverClassName=com.mysql.jdbc.Driver
spring.datasource.username=root
